<% include ../partials/header%>

<div class="row">
    <h1>Create a Product with SQL</h1>

    <div class="grid-container">
        <form accept-charset="UTF-8" action="/createsql" method="post">

            <div class="item">
                <label for="name">Name:</label>
                <input name="name" type="text" placeholder="Please enter product name" />
                
                <label for="price">Price:</label>
                <input name="price" type="text" placeholder="Please enter your email address" />     

                <label for="description">Description:</label>
                <input name="comment" type="text" placeholder="Please enter Description" /> 
                
                 <label for="image">Image:</label>
                <input name="image" type="text" placeholder="Please enter image name" /> 

                <button class="button">Create Service</button>
            </div>


<% include ../partials/footer%>