<% include ../partials/header%>

<div class="row">
    <h1>Edit SQL page</h1>

    <div class="grid-container">
        
        <% res1.forEach(function(services){ %>
        
        <form accept-charset="UTF-8" action="/editsql/<%=services.Id%>" method="post">

            <div class="item">
                <label for="name">Name:</label>
                <input name="name" type="text" value="<%= services.Name %>" />
                
                <label for="price">Price:</label>
                <input name="price" type="text" vailue="<%= services.Price %>" />     

                <label for="description">Description:</label>
                <input name="description" type="text" value="<%= services.Description %>" /> 
                
                 <label for="image">Description:</label>
                <input name="image" type="text"value="<%= services.Image %>" /> 

                <button class="button">Edit Service</button>
              </div>
              </form>
              <% }); %>
    </div>
 
</div>


<% include ../partials/footer%>